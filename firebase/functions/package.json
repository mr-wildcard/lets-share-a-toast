    "firebase-functions": "4.0.2"
  },
  "devDependencies": {
    "esbuild": "0.14.14",
    "firebase-functions-test": "3.0.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "typescript": "4.5.5"
  }
}
